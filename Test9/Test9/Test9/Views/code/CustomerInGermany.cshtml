
@{
    ViewBag.Title = "CustomerInGermany";
}



@model List<test9.Models.Customer> 

<h2>Customers In the Germany</h2>

<table>
    <tr>
        <th>Customer ID</th>
        <th>Company Name</th>
        <th>Contact Name</th>    </tr>
    @foreach (var customer in Model)
    {
<tr>
    <td>@customer.CustomerID</td>
    <td>@customer.CompanyName</td>
    <td>@customer.ContactName</td>
</tr>}
</table>